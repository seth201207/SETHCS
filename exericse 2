print(6 * (1 - 2))

bruce = 6
print(bruce + 4)

P = 10000
n = 12
r = 8/100
t = int(input("How many years?"))
print((P*(1+(r/n))**t))

time = int(input("What is the time "))
alarm = int(input("how long u wanna sleep bro"))
end = time + alarm
if end > 24:
    woke = end % 24
    woke = time - woke
    print(woke)
